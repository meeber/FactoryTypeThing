{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {"sourceType": "module"},
  "rules": {
    "comma-dangle": [2, "always-multiline"],
    "indent": [2, 2],
    "linebreak-style": [2, "unix"],
    "no-console": 1,
    "no-unused-vars": 1,
    "quotes": [2, "double"],
    "semi": [2, "always"],
    "strict": [2, "global"]
  }
}
